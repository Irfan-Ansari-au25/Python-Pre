    "def print_substrings(s):\n",
    "    for i in range(len(s)):\n",
    "        for j in range(i+1,len(s)):\n",
    "            print(s[i:j])\n",
    "            \n",
    "if __name__ == '__main__':\n",
    "    s = 'adobecodebanc'\n",
    "    print_substrings(s)"
